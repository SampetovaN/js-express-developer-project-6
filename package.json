{
   "name": "@hexlet/code",
   "version": "0.1.0",
   "type": "module",
   "description": "@hexlet/code",
   "engines": {
      "node": ">=16.x"
   },
   "scripts": {
      "prebuild": "rm -rf dist",
      "test": "NODE_ENV=test jest --verbose --runInBand",
      "build": "webpack",
      "build-db": "cp knexfile.js dist && knex migrate:latest",
      "start": "fastify start -p 10000 -a 0.0.0.0 server/plugin.js -l debug -P -o",
      "start-production": "npm run build-db && npm run build && npm run start"
   },
   "jest": {
      "modulePathIgnorePatterns": [
         "<rootDir>/__tests__/helpers/"
      ],
      "setupFiles": [
         "dotenv/config"
      ]
   },
   "main": "server/plugin.js",
   "author": "Nika Sampetova",
   "dependencies": {
      "@fastify/auth": "^4.4.0",
      "@fastify/formbody": "^7.0.1",
      "@fastify/passport": "^2.3.0",
      "@fastify/secure-session": "^5.2.0",
      "@fastify/sensible": "^5.1.0",
      "@fastify/static": "^6.5.0",
      "@fastify/view": "^7.1.0",
      "@vscode/sqlite3": "^5.0.8",
      "bootstrap": "^5.2.0",
      "fastify": "^4.10.2",
      "fastify-cli": "^5.1.0",
      "fastify-error-page": "^4.0.0",
      "fastify-method-override": "^1.5.9",
      "fastify-objectionjs": "^2.0.1",
      "fastify-reverse-routes": "^3.0.0",
      "i18next": "^21.9.1",
      "jquery": "^3.6.0",
      "lodash": "^4.17.21",
      "mini-css-extract-plugin": "^2.6.1",
      "npm-check-updates": "^16.0.5",
      "objection": "^3.0.1",
      "objection-unique": "^1.2.2",
      "pg": "^8.11.3",
      "pino-pretty": "^9.0.1",
      "pug": "^3.0.2",
      "qs": "^6.11.0",
      "simple-knex-fixtures": "^2.0.3",
      "sqlite3": "^5.1.7",
      "webpack": "^5.74.0",
      "dotenv": "^16.0.1"
   },
   "devDependencies": {
      "@faker-js/faker": "^8.4.0",
      "@jest/globals": "^28.1.3",
      "@types/jest": "^28.1.7",
      "@types/node": "^18.7.11",
      "css-loader": "^6.7.1",
      "eslint": "^8.22.0",
      "eslint-config-airbnb-base": "^15.0.0",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-jest": "^26.8.7",
      "jest": "^28.1.3",
      "jest-cli": "^28.1.3",
      "pino-pretty": "^9.0.1",
      "style-loader": "^3.3.1",
      "webpack-cli": "^4.10.0"
   }
}
